{
  "name": "@alexanderfedin/async-queue",
  "version": "1.0.0",
  "description": "TypeScript async producer-consumer queue with backpressure control, similar to Go channels or .NET Channel<T>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist coverage",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build && npm test",
    "test": "jest --testPathIgnorePatterns=stress",
    "test:stress": "jest test/*.stress.test.ts --verbose --testTimeout=60000",
    "test:all": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "example": "ts-node examples/basic.ts",
    "example:backpressure": "ts-node examples/backpressure.ts",
    "example:multi": "ts-node examples/multiple-workers.ts",
    "benchmark": "ts-node benchmark/src/throughput.ts",
    "benchmark:compare": "ts-node benchmark/src/compare-implementations.ts",
    "benchmark:native": "ts-node benchmark/src/compare-native.ts",
    "benchmark:test": "jest benchmark/test/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/o2alexanderfedin/async-queue-js.git"
  },
  "keywords": [
    "async",
    "queue",
    "backpressure",
    "producer-consumer",
    "channel",
    "concurrent",
    "push-pull",
    "flow-control",
    "promise",
    "async-await",
    "blocking-queue",
    "bounded-queue"
  ],
  "author": "Alexander Fedin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/o2alexanderfedin/async-queue-js/issues"
  },
  "homepage": "https://github.com/o2alexanderfedin/async-queue-js#readme",
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
